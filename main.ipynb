    "wsg_src = builder.AddSystem(ConstantVectorSource([1.0]))\n",
    "builder.Connect(wsg_src.get_output_port(), station.GetInputPort(\"wsg.position\"))\n",
    "\n",
    "diagram = builder.Build()\n"
